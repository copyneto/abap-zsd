FORM GET_DATA_ZVSD_ICMSDIF.
  PERFORM VIM_FILL_WHERETAB.
*.read data from database.............................................*
  REFRESH TOTAL.
  CLEAR   TOTAL.
  SELECT * FROM ZTSD_ICMSDIF WHERE
(VIM_WHERETAB) .
    CLEAR ZVSD_ICMSDIF .
ZVSD_ICMSDIF-MANDT =
ZTSD_ICMSDIF-MANDT .
ZVSD_ICMSDIF-UF_EMIS =
ZTSD_ICMSDIF-UF_EMIS .
ZVSD_ICMSDIF-UF_RECE =
ZTSD_ICMSDIF-UF_RECE .
ZVSD_ICMSDIF-MATNR =
ZTSD_ICMSDIF-MATNR .
ZVSD_ICMSDIF-MATKL =
ZTSD_ICMSDIF-MATKL .
ZVSD_ICMSDIF-PORCENTAGEM =
ZTSD_ICMSDIF-PORCENTAGEM .
ZVSD_ICMSDIF-DATE_FROM =
ZTSD_ICMSDIF-DATE_FROM .
ZVSD_ICMSDIF-DATE_TO =
ZTSD_ICMSDIF-DATE_TO .
<VIM_TOTAL_STRUC> = ZVSD_ICMSDIF.
    APPEND TOTAL.
  ENDSELECT.
  SORT TOTAL BY <VIM_XTOTAL_KEY>.
  <STATUS>-ALR_SORTED = 'R'.
*.check dynamic selectoptions (not in DDIC)...........................*
  IF X_HEADER-SELECTION NE SPACE.
    PERFORM CHECK_DYNAMIC_SELECT_OPTIONS.
  ELSEIF X_HEADER-DELMDTFLAG NE SPACE.
    PERFORM BUILD_MAINKEY_TAB.
  ENDIF.
  REFRESH EXTRACT.
ENDFORM.
*---------------------------------------------------------------------*
FORM DB_UPD_ZVSD_ICMSDIF .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO ZVSD_ICMSDIF.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_ZVSD_ICMSDIF-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_ICMSDIF WHERE
  UF_EMIS = ZVSD_ICMSDIF-UF_EMIS AND
  UF_RECE = ZVSD_ICMSDIF-UF_RECE AND
  MATNR = ZVSD_ICMSDIF-MATNR AND
  MATKL = ZVSD_ICMSDIF-MATKL .
    IF SY-SUBRC = 0.
    DELETE ZTSD_ICMSDIF .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_ICMSDIF WHERE
  UF_EMIS = ZVSD_ICMSDIF-UF_EMIS AND
  UF_RECE = ZVSD_ICMSDIF-UF_RECE AND
  MATNR = ZVSD_ICMSDIF-MATNR AND
  MATKL = ZVSD_ICMSDIF-MATKL .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR ZTSD_ICMSDIF.
    ENDIF.
ZTSD_ICMSDIF-MANDT =
ZVSD_ICMSDIF-MANDT .
ZTSD_ICMSDIF-UF_EMIS =
ZVSD_ICMSDIF-UF_EMIS .
ZTSD_ICMSDIF-UF_RECE =
ZVSD_ICMSDIF-UF_RECE .
ZTSD_ICMSDIF-MATNR =
ZVSD_ICMSDIF-MATNR .
ZTSD_ICMSDIF-MATKL =
ZVSD_ICMSDIF-MATKL .
ZTSD_ICMSDIF-PORCENTAGEM =
ZVSD_ICMSDIF-PORCENTAGEM .
ZTSD_ICMSDIF-DATE_FROM =
ZVSD_ICMSDIF-DATE_FROM .
ZTSD_ICMSDIF-DATE_TO =
ZVSD_ICMSDIF-DATE_TO .
    IF SY-SUBRC = 0.
    UPDATE ZTSD_ICMSDIF ##WARN_OK.
    ELSE.
    INSERT ZTSD_ICMSDIF .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_ZVSD_ICMSDIF-UPD_FLAG,
STATUS_ZVSD_ICMSDIF-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
