@EndUserText.label: 'CDS de Autorização'
@MappingRole: true
define role ZI_SD_INCL_USER {
    grant 
        select
            on
                ZI_SD_INCL_USER
                    where
                        ( ) = aspect pfcg_auth( ZSDMTABLE, ACTVT = '03', TABLE = 'ZTSD_INCL_USER' );
                        
}