MOVE ZVMM_MOV_PARAM-MATKL TO
ZTMM_MOV_PARAM-MATKL .
MOVE ZVMM_MOV_PARAM-TAXLW1 TO
ZTMM_MOV_PARAM-TAXLW1 .
MOVE ZVMM_MOV_PARAM-TAXLW2 TO
ZTMM_MOV_PARAM-TAXLW2 .
MOVE ZVMM_MOV_PARAM-TAXLW5 TO
ZTMM_MOV_PARAM-TAXLW5 .
MOVE ZVMM_MOV_PARAM-TAXLW4 TO
ZTMM_MOV_PARAM-TAXLW4 .
MOVE ZVMM_MOV_PARAM-TAXSIT TO
ZTMM_MOV_PARAM-TAXSIT .
MOVE ZVMM_MOV_PARAM-MANDT TO
ZTMM_MOV_PARAM-MANDT .
  CORR_KEYTAB             =  E071K.
  CORR_KEYTAB-OBJNAME     = 'ZTMM_MOV_PARAM'.
  IF NOT <vim_corr_keyx> IS ASSIGNED.
    ASSIGN CORR_KEYTAB-TABKEY TO <vim_corr_keyx> CASTING.
  ENDIF.
  ASSIGN ZTMM_MOV_PARAM TO <TAB_KEY_X> CASTING.
  PERFORM VIM_GET_TRSPKEYLEN
    USING 'ZTMM_MOV_PARAM'
    CHANGING TRSP_KEYLEN.
  <VIM_CORR_KEYX>(TRSP_KEYLEN) = <TAB_KEY_X>(TRSP_KEYLEN).
  PERFORM UPDATE_CORR_KEYTAB USING CM_ACTION RETCODE.
  ADD: RETCODE TO RC, 1 TO COUNT.
  IF RC LT COUNT AND CM_ACTION NE PRUEFEN.
    CLEAR RC.
  ENDIF.

ENDFORM.
