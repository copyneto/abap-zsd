ZTMM_MOV_PARAM-MATKL .
ZVMM_MOV_PARAM-TAXLW1 =
ZTMM_MOV_PARAM-TAXLW1 .
ZVMM_MOV_PARAM-TAXLW2 =
ZTMM_MOV_PARAM-TAXLW2 .
ZVMM_MOV_PARAM-TAXLW5 =
ZTMM_MOV_PARAM-TAXLW5 .
ZVMM_MOV_PARAM-TAXLW4 =
ZTMM_MOV_PARAM-TAXLW4 .
ZVMM_MOV_PARAM-TAXSIT =
ZTMM_MOV_PARAM-TAXSIT .
ZVMM_MOV_PARAM-ATIVO =
ZTMM_MOV_PARAM-ATIVO .
<VIM_TOTAL_STRUC> = ZVMM_MOV_PARAM.
    APPEND TOTAL.
  ENDSELECT.
  SORT TOTAL BY <VIM_XTOTAL_KEY>.
  <STATUS>-ALR_SORTED = 'R'.
*.check dynamic selectoptions (not in DDIC)...........................*
  IF X_HEADER-SELECTION NE SPACE.
    PERFORM CHECK_DYNAMIC_SELECT_OPTIONS.
  ELSEIF X_HEADER-DELMDTFLAG NE SPACE.
    PERFORM BUILD_MAINKEY_TAB.
  ENDIF.
  REFRESH EXTRACT.
ENDFORM.
*---------------------------------------------------------------------*
FORM DB_UPD_ZVMM_MOV_PARAM .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO ZVMM_MOV_PARAM.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_ZVMM_MOV_PARAM-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZTMM_MOV_PARAM WHERE
  SHIPFROM = ZVMM_MOV_PARAM-SHIPFROM AND
  DIRECAO = ZVMM_MOV_PARAM-DIRECAO AND
  CFOP = ZVMM_MOV_PARAM-CFOP AND
  MATNR = ZVMM_MOV_PARAM-MATNR AND
  MATKL = ZVMM_MOV_PARAM-MATKL AND
  TAXLW1 = ZVMM_MOV_PARAM-TAXLW1 AND
  TAXLW2 = ZVMM_MOV_PARAM-TAXLW2 AND
  TAXLW5 = ZVMM_MOV_PARAM-TAXLW5 AND
  TAXLW4 = ZVMM_MOV_PARAM-TAXLW4 AND
  TAXSIT = ZVMM_MOV_PARAM-TAXSIT .
    IF SY-SUBRC = 0.
    DELETE ZTMM_MOV_PARAM .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM ZTMM_MOV_PARAM WHERE
  SHIPFROM = ZVMM_MOV_PARAM-SHIPFROM AND
  DIRECAO = ZVMM_MOV_PARAM-DIRECAO AND
  CFOP = ZVMM_MOV_PARAM-CFOP AND
  MATNR = ZVMM_MOV_PARAM-MATNR AND
  MATKL = ZVMM_MOV_PARAM-MATKL AND
  TAXLW1 = ZVMM_MOV_PARAM-TAXLW1 AND
  TAXLW2 = ZVMM_MOV_PARAM-TAXLW2 AND
  TAXLW5 = ZVMM_MOV_PARAM-TAXLW5 AND
  TAXLW4 = ZVMM_MOV_PARAM-TAXLW4 AND
  TAXSIT = ZVMM_MOV_PARAM-TAXSIT .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR ZTMM_MOV_PARAM.
    ENDIF.
ZTMM_MOV_PARAM-MANDT =
ZVMM_MOV_PARAM-MANDT .
ZTMM_MOV_PARAM-SHIPFROM =
ZVMM_MOV_PARAM-SHIPFROM .
ZTMM_MOV_PARAM-DIRECAO =
ZVMM_MOV_PARAM-DIRECAO .
ZTMM_MOV_PARAM-CFOP =
ZVMM_MOV_PARAM-CFOP .
ZTMM_MOV_PARAM-MATNR =
ZVMM_MOV_PARAM-MATNR .
ZTMM_MOV_PARAM-MATKL =
ZVMM_MOV_PARAM-MATKL .
ZTMM_MOV_PARAM-TAXLW1 =
ZVMM_MOV_PARAM-TAXLW1 .
ZTMM_MOV_PARAM-TAXLW2 =
ZVMM_MOV_PARAM-TAXLW2 .
ZTMM_MOV_PARAM-TAXLW5 =
ZVMM_MOV_PARAM-TAXLW5 .
ZTMM_MOV_PARAM-TAXLW4 =
ZVMM_MOV_PARAM-TAXLW4 .
ZTMM_MOV_PARAM-TAXSIT =
ZVMM_MOV_PARAM-TAXSIT .
ZTMM_MOV_PARAM-ATIVO =
ZVMM_MOV_PARAM-ATIVO .
    IF SY-SUBRC = 0.
    UPDATE ZTMM_MOV_PARAM ##WARN_OK.
    ELSE.
    INSERT ZTMM_MOV_PARAM .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_ZVMM_MOV_PARAM-UPD_FLAG,
STATUS_ZVMM_MOV_PARAM-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
