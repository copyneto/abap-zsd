  ELSEIF X_HEADER-DELMDTFLAG NE SPACE.
    PERFORM BUILD_MAINKEY_TAB.
  ENDIF.
  REFRESH EXTRACT.
ENDFORM.
*---------------------------------------------------------------------*
FORM DB_UPD_ZVSD_GNRET005 .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO ZVSD_GNRET005.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_ZVSD_GNRET005-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_GNRET005 WHERE
  SHIPFROM = ZVSD_GNRET005-SHIPFROM AND
  SHIPTO = ZVSD_GNRET005-SHIPTO AND
  CONSUMO = ZVSD_GNRET005-CONSUMO .
    IF SY-SUBRC = 0.
    DELETE ZTSD_GNRET005 .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_GNRET005 WHERE
  SHIPFROM = ZVSD_GNRET005-SHIPFROM AND
  SHIPTO = ZVSD_GNRET005-SHIPTO AND
  CONSUMO = ZVSD_GNRET005-CONSUMO .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR ZTSD_GNRET005.
    ENDIF.
ZTSD_GNRET005-MANDT =
ZVSD_GNRET005-MANDT .
ZTSD_GNRET005-SHIPFROM =
ZVSD_GNRET005-SHIPFROM .
ZTSD_GNRET005-SHIPTO =
ZVSD_GNRET005-SHIPTO .
ZTSD_GNRET005-CONSUMO =
ZVSD_GNRET005-CONSUMO .
ZTSD_GNRET005-TPGUIA =
ZVSD_GNRET005-TPGUIA .
ZTSD_GNRET005-MODO =
ZVSD_GNRET005-MODO .
ZTSD_GNRET005-GUIA_POR_RECEITA =
ZVSD_GNRET005-GUIA_POR_RECEITA .
ZTSD_GNRET005-ZSUB =
ZVSD_GNRET005-ZSUB .
ZTSD_GNRET005-APURACAO =
ZVSD_GNRET005-APURACAO .
ZTSD_GNRET005-DESTINO =
ZVSD_GNRET005-DESTINO .
    IF SY-SUBRC = 0.
    UPDATE ZTSD_GNRET005 ##WARN_OK.
    ELSE.
    INSERT ZTSD_GNRET005 .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_ZVSD_GNRET005-UPD_FLAG,
STATUS_ZVSD_GNRET005-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
