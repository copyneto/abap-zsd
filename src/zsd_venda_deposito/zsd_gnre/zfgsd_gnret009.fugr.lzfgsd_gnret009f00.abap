  ENDIF.
  REFRESH EXTRACT.
ENDFORM.
*---------------------------------------------------------------------*
FORM DB_UPD_ZVSD_GNRET009 .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO ZVSD_GNRET009.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_ZVSD_GNRET009-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_GNRET009 WHERE
  BUKRS = ZVSD_GNRET009-BUKRS AND
  BRANCH = ZVSD_GNRET009-BRANCH AND
  SHIPTO = ZVSD_GNRET009-SHIPTO AND
  TAXTYP = ZVSD_GNRET009-TAXTYP AND
  ZSUB = ZVSD_GNRET009-ZSUB AND
  ADIANTAMENTO = ZVSD_GNRET009-ADIANTAMENTO .
    IF SY-SUBRC = 0.
    DELETE ZTSD_GNRET009 .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_GNRET009 WHERE
  BUKRS = ZVSD_GNRET009-BUKRS AND
  BRANCH = ZVSD_GNRET009-BRANCH AND
  SHIPTO = ZVSD_GNRET009-SHIPTO AND
  TAXTYP = ZVSD_GNRET009-TAXTYP AND
  ZSUB = ZVSD_GNRET009-ZSUB AND
  ADIANTAMENTO = ZVSD_GNRET009-ADIANTAMENTO .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR ZTSD_GNRET009.
    ENDIF.
ZTSD_GNRET009-MANDT =
ZVSD_GNRET009-MANDT .
ZTSD_GNRET009-BUKRS =
ZVSD_GNRET009-BUKRS .
ZTSD_GNRET009-BRANCH =
ZVSD_GNRET009-BRANCH .
ZTSD_GNRET009-SHIPTO =
ZVSD_GNRET009-SHIPTO .
ZTSD_GNRET009-TAXTYP =
ZVSD_GNRET009-TAXTYP .
ZTSD_GNRET009-ZSUB =
ZVSD_GNRET009-ZSUB .
ZTSD_GNRET009-ADIANTAMENTO =
ZVSD_GNRET009-ADIANTAMENTO .
ZTSD_GNRET009-LIFNR =
ZVSD_GNRET009-LIFNR .
ZTSD_GNRET009-HKONT =
ZVSD_GNRET009-HKONT .
ZTSD_GNRET009-HBKID =
ZVSD_GNRET009-HBKID .
    IF SY-SUBRC = 0.
    UPDATE ZTSD_GNRET009 ##WARN_OK.
    ELSE.
    INSERT ZTSD_GNRET009 .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_ZVSD_GNRET009-UPD_FLAG,
STATUS_ZVSD_GNRET009-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
