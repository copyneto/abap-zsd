FORM GET_DATA_ZVSD_GNRET011.
  PERFORM VIM_FILL_WHERETAB.
*.read data from database.............................................*
  REFRESH TOTAL.
  CLEAR   TOTAL.
  SELECT * FROM ZTSD_GNRET011 WHERE
(VIM_WHERETAB) .
    CLEAR ZVSD_GNRET011 .
ZVSD_GNRET011-MANDT =
ZTSD_GNRET011-MANDT .
ZVSD_GNRET011-DOCNUM =
ZTSD_GNRET011-DOCNUM .
ZVSD_GNRET011-ACAO =
ZTSD_GNRET011-ACAO .
ZVSD_GNRET011-CREDAT =
ZTSD_GNRET011-CREDAT .
ZVSD_GNRET011-CRETIM =
ZTSD_GNRET011-CRETIM .
ZVSD_GNRET011-CRENAM =
ZTSD_GNRET011-CRENAM .
<VIM_TOTAL_STRUC> = ZVSD_GNRET011.
    APPEND TOTAL.
  ENDSELECT.
  SORT TOTAL BY <VIM_XTOTAL_KEY>.
  <STATUS>-ALR_SORTED = 'R'.
*.check dynamic selectoptions (not in DDIC)...........................*
  IF X_HEADER-SELECTION NE SPACE.
    PERFORM CHECK_DYNAMIC_SELECT_OPTIONS.
  ELSEIF X_HEADER-DELMDTFLAG NE SPACE.
    PERFORM BUILD_MAINKEY_TAB.
  ENDIF.
  REFRESH EXTRACT.
ENDFORM.
*---------------------------------------------------------------------*
FORM DB_UPD_ZVSD_GNRET011 .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO ZVSD_GNRET011.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_ZVSD_GNRET011-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_GNRET011 WHERE
  DOCNUM = ZVSD_GNRET011-DOCNUM AND
  ACAO = ZVSD_GNRET011-ACAO .
    IF SY-SUBRC = 0.
    DELETE ZTSD_GNRET011 .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_GNRET011 WHERE
  DOCNUM = ZVSD_GNRET011-DOCNUM AND
  ACAO = ZVSD_GNRET011-ACAO .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR ZTSD_GNRET011.
    ENDIF.
ZTSD_GNRET011-MANDT =
ZVSD_GNRET011-MANDT .
ZTSD_GNRET011-DOCNUM =
ZVSD_GNRET011-DOCNUM .
ZTSD_GNRET011-ACAO =
ZVSD_GNRET011-ACAO .
ZTSD_GNRET011-CREDAT =
ZVSD_GNRET011-CREDAT .
ZTSD_GNRET011-CRETIM =
ZVSD_GNRET011-CRETIM .
ZTSD_GNRET011-CRENAM =
ZVSD_GNRET011-CRENAM .
    IF SY-SUBRC = 0.
    UPDATE ZTSD_GNRET011 ##WARN_OK.
    ELSE.
    INSERT ZTSD_GNRET011 .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_ZVSD_GNRET011-UPD_FLAG,
STATUS_ZVSD_GNRET011-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
