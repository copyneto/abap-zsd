ZVSD_MATERIAL-AGREGADO .
ZTSD_MATERIAL-ICMS_DEST =
ZVSD_MATERIAL-ICMS_DEST .
ZTSD_MATERIAL-ICMS_ORIG =
ZVSD_MATERIAL-ICMS_ORIG .
ZTSD_MATERIAL-COMPRA_INTERNA =
ZVSD_MATERIAL-COMPRA_INTERNA .
ZTSD_MATERIAL-BASE_RED_ORIG =
ZVSD_MATERIAL-BASE_RED_ORIG .
ZTSD_MATERIAL-BASE_RED_DEST =
ZVSD_MATERIAL-BASE_RED_DEST .
ZTSD_MATERIAL-TAXA_FCP =
ZVSD_MATERIAL-TAXA_FCP .
ZTSD_MATERIAL-ICMS_EFET =
ZVSD_MATERIAL-ICMS_EFET .
ZTSD_MATERIAL-BASEREDEFET =
ZVSD_MATERIAL-BASEREDEFET .
ZTSD_MATERIAL-PRECO_COMPAR =
ZVSD_MATERIAL-PRECO_COMPAR .
ZTSD_MATERIAL-PRECO_PAUTA =
ZVSD_MATERIAL-PRECO_PAUTA .
ZTSD_MATERIAL-AGREGADO_PAUTA =
ZVSD_MATERIAL-AGREGADO_PAUTA .
ZTSD_MATERIAL-NRO_UNIDS =
ZVSD_MATERIAL-NRO_UNIDS .
ZTSD_MATERIAL-UM =
ZVSD_MATERIAL-UM .
ZTSD_MATERIAL-MODALIDADE =
ZVSD_MATERIAL-MODALIDADE .
ZTSD_MATERIAL-CALC_EFETIVO =
ZVSD_MATERIAL-CALC_EFETIVO .
ZTSD_MATERIAL-PERC_BC_ICMS =
ZVSD_MATERIAL-PERC_BC_ICMS .
    IF SY-SUBRC = 0.
    UPDATE ZTSD_MATERIAL ##WARN_OK.
    ELSE.
    INSERT ZTSD_MATERIAL .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_ZVSD_MATERIAL-UPD_FLAG,
STATUS_ZVSD_MATERIAL-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
