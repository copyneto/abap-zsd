*---------------------------------------------------------------------*
FORM DB_UPD_ZVSD_MATERIAL .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO ZVSD_MATERIAL.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_ZVSD_MATERIAL-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_MATERIAL WHERE
  CENTRO = ZVSD_MATERIAL-CENTRO AND
  UF = ZVSD_MATERIAL-UF AND
  MATERIAL = ZVSD_MATERIAL-MATERIAL .
    IF SY-SUBRC = 0.
    DELETE ZTSD_MATERIAL .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM ZTSD_MATERIAL WHERE
  CENTRO = ZVSD_MATERIAL-CENTRO AND
  UF = ZVSD_MATERIAL-UF AND
  MATERIAL = ZVSD_MATERIAL-MATERIAL .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR ZTSD_MATERIAL.
    ENDIF.
ZTSD_MATERIAL-CLIENT =
ZVSD_MATERIAL-CLIENT .
ZTSD_MATERIAL-CENTRO =
ZVSD_MATERIAL-CENTRO .
ZTSD_MATERIAL-UF =
ZVSD_MATERIAL-UF .
ZTSD_MATERIAL-MATERIAL =
ZVSD_MATERIAL-MATERIAL .
