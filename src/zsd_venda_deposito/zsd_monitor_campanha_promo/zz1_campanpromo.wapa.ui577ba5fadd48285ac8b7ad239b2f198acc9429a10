sap.ui.define([                                                                                                                                                                                                                                                
    "sap/ui/core/mvc/Controller",                                                                                                                                                                                                                              
	"sap/m/MessageBox",                                                                                                                                                                                                                                           
	"sap/m/MessageToast"                                                                                                                                                                                                                                          
],                                                                                                                                                                                                                                                             
function (MessageBox){                                                                                                                                                                                                                                         
    "use strict";                                                                                                                                                                                                                                              
    return sap.ui.controller("br.com.trescoracoes.monitorcampanhapromocional.ext.controller.ListReportExt",{&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                               
        onMateriaisPromo: function(oEvent) {                                                                                                                                                                                                                   
             /* ===================================================================&nbsp;                                                                                                                                                                      
                                Chama outro aplicativo                                                                                                                                                                                                         
                =================================================================== */                                                                                                                                                                         
                                var href_material = (sap.ushell && sap.ushell.Container &&                                                                                                                                                                     
                                    sap.ushell.Container.getService("CrossApplicationNavigation").hrefForExternal({                                                                                                                                            
                                        target: { semanticObject: "monitor_campanha", action: "create" },                                                                                                                                                      
                                    }));                                                                                                                                                                                                                       
                                sap.m.URLHelper.redirect(href_material, true);                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
        },                                                                                                                                                                                                                                                     
        onFuncaoBP: function(oEvent) {                                                                                                                                                                                                                         
            /* ===================================================================&nbsp;                                                                                                                                                                       
                                Chama outro aplicativo                                                                                                                                                                                                         
                =================================================================== */                                                                                                                                                                         
                var href_funcao = (sap.ushell && sap.ushell.Container &&                                                                                                                                                                                       
                    sap.ushell.Container.getService("CrossApplicationNavigation").hrefForExternal({                                                                                                                                                            
                        target: { semanticObject: "ionz_parceiros", action: "create" },                                                                                                                                                                        
                    }));                                                                                                                                                                                                                                       
                sap.m.URLHelper.redirect(href_funcao, true);                                                                                                                                                                                                   
        }                                                                                                                                                                                                                                                      
    });                                                                                                                                                                                                                                                        
});                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               