sap.ui.define([],                                                                                                                                                                                                                                              
    function () {                                                                                                                                                                                                                                              
        "use strict";                                                                                                                                                                                                                                          
        return sap.ui.controller( "br.com.trescoracoes.coligados.ext.controller.ListReportExt", {                                                                                                                                                              
                                                                                                                                                                                                                                                               
            onAfterRendering: function (oEvent) {                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
                let oControl = this.getView().byId("addEntry");                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
                let oText = this.getOwnerComponent().getModel("i18n").getResourceBundle();                                                                                                                                                                     
                                                                                                                                                                                                                                                               
                oControl.setText(oText.getText("create"))                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
            }                                                                                                                                                                                                                                                  
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                               
        }                                                                                                                                                                                                                                                      
        );                                                                                                                                                                                                                                                     
    });                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               